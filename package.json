{
  "name": "@hlolli/react-tabtab",
  "version": "2.1.1",
  "description": "A mobile support, draggable, editable and api based Tab for ReactJS",
  "main": "lib/",
  "scripts": {
    "start": "node devServer.js",
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "rimraf ./lib && cross-env BABEL_ENV=production babel src -d lib",
    "build:dist": "rimraf dist && rollup -c --environment ESBUNDLE && rollup -c --environment PRODUCTION",
    "lint": "eslint src example test",
    "test": "jest",
    "test:watch": "NODE_ENV=test npm test -- --watch",
    "flow": "flow check",
    "flow:watch": "flow-watch",
    "gh-pages": "rimraf _gh-pages && npm run gh-pages:build && npm run gh-pages:publish",
    "gh-pages:build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "gh-pages:publish": "git-directory-deploy --directory _gh-pages --branch gh-pages",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctxhou/react-tabtab.git"
  },
  "keywords": [
    "react",
    "tabs",
    "react-tab",
    "react-tabtab",
    "react-component",
    "tab",
    "tabtab",
    "styled-components"
  ],
  "files": [
    "dist",
    "docs",
    "flow-typed",
    "lib",
    "src"
  ],
  "author": "ctxhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctxhou/react-tabtab/issues"
  },
  "homepage": "https://github.com/ctxhou/react-tabtab#readme",
  "dependencies": {
    "array-move": "^3.0.1",
    "invariant": "^2.2.4",
    "react-test-renderer": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-flow-react-proptypes": "^26.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.2",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-flowtype-errors": "^4.3.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.134.0",
    "flow-watch": "^2.0.0",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "noop3": "^1000.0.0",
    "precommit-hook-eslint": "^3.0.0",
    "prettier": "^2.1.2",
    "react-content-loader": "^5.1.1",
    "react-icons": "^3.11.0",
    "react-select": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": ">=16.13.1",
    "react-poppop": "^1.5.0",
    "react-sortable-hoc": "^1.11.0",
    "styled-components": "^5.2.0"
  },
  "jest": {
    "setupFiles": [
      "./test/shim",
      "./test/enzyme-setup"
    ],
    "roots": [
      "<rootDir>/test/"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
